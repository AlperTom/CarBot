<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Enhanced gradients for professional appearance -->
    <linearGradient id="brandGradientFav" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#F59E0B;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#F97316;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#EF4444;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="containerBgFav" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E293B;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#334155;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Professional container -->
  <rect x="2" y="2" width="28" height="28" rx="6" fill="url(#containerBgFav)"/>
  
  <!-- Refined Car Icon -->
  <g transform="translate(8, 10)">
    <!-- Car body -->
    <path d="M1 8 Q1 7 2 7 L14 7 Q15 7 15 8 L15 10 Q15 11 14 11 L2 11 Q1 11 1 10 Z" 
          fill="url(#brandGradientFav)" opacity="0.95"/>
    
    <!-- Windshield -->
    <path d="M3 7 Q4 5 6 5 L10 5 Q12 5 13 7" 
          fill="url(#brandGradientFav)" opacity="0.7"/>
    
    <!-- Wheels -->
    <circle cx="4" cy="11" r="1.5" fill="white" stroke="url(#brandGradientFav)" stroke-width="0.8"/>
    <circle cx="12" cy="11" r="1.5" fill="white" stroke="url(#brandGradientFav)" stroke-width="0.8"/>
    
    <!-- Wheel centers -->
    <circle cx="4" cy="11" r="0.8" fill="url(#brandGradientFav)"/>
    <circle cx="12" cy="11" r="0.8" fill="url(#brandGradientFav)"/>
    
    <!-- AI chip -->
    <rect x="6" y="4" width="4" height="3" rx="0.5" fill="white" opacity="0.9" 
          stroke="url(#brandGradientFav)" stroke-width="0.5"/>
    <circle cx="8" cy="5.5" r="0.8" fill="url(#brandGradientFav)"/>
    <circle cx="8" cy="5.5" r="0.4" fill="white" opacity="0.8"/>
  </g>
</svg>